<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    id="Rory-Cordova-Plugin" version="0.0.1">
    <name>Rory-Cordova-Plugin</name>
    <description>Cordova Device Plugin</description>
    <license>Apache 2.0</license>
    <keywords>cordova,device</keywords>
    <js-module src="www/device.js" name="device">
        <clobbers target="device" />
    </js-module>
    <platform name="android">
        <config-file target="config.xml" parent="/*">
            <feature name="Rory">
                <param name="android-package" value="com.example.rory.Rory"/>
            </feature>
        </config-file>
        <source-file src="src/android/com/example/rory/Rory.kt" target-dir="src/android/com/example/rory"/>
    </platform>
</plugin>